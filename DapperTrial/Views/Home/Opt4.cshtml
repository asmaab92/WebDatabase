@{
}

@Model List<City>
    @{
        ViewData["Title"] = "Palestinian cities Page";
    }

    <div class="text-center">
        <h1 class="display-4">This page presents the cities in the country Palestine</h1>
    </div>

    <table>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>CountryCode</td>
            <td>District</td>
            <td>Population</td>
        </tr>
        @foreach (City city in @Model)
        {
            <tr>
                <td>@city.Id</td>
                <td>@city.Name</td>
                <td>@city.CountryCode</td>
                <td>@city.District</td>
                <td>@city.Population</td>
            </tr>
        }

    </table>

